<!doctype html>
<html lang="en" data-ng-app="ChaChingApp">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="{{app.description}}">
    <meta name="keywords" content="app, responsive, angular, bootstrap, dashboard, admin">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta name="HandheldFriendly" content="true" />
    <meta name="apple-touch-fullscreen" content="yes" />
    <title data-ng-bind="pageTitle()">ChaChing 123 - Admin Page</title>
    <!-- Google fonts -->
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,400italic,600,700|Raleway:300,400,500,600,700|Crete+Round:400italic" rel="stylesheet" type="text/css" />
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Themify Icons -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/bower_components/themify-icons/css/themify-icons.css">
    <!-- Loading Bar -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-loading-bar/build/loading-bar.min.css">
    <!-- Animate Css -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/bower_components/animate.css/animate.min.css">
    <!--Toastr - Notification Message-->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/vendors/css/toastr.css">
    <!-- Clip-Two CSS -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/assets/css/styles.css">
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/assets/css/plugins.css">
    <!-- Clip-Two Theme -->
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/assets/css/themes/theme-1.css" />
    <link rel="stylesheet" href="~/Areas/Admin/Scripts/spa_admin/assets/css/styles.chaching.css" />
    <link rel="icon" type="image/png" sizes="96x96" href="~/Areas/Admin/Scripts/spa_admin/assets/images/favicon-96x96.png">
</head>
<body ng-controller="AppCtrl">
    @Html.AntiForgeryToken()
    <input id="__virtualDirectory" name="__virtualDirectory" type="hidden" value="@ViewBag.__virtualDirectory" />
    <input id="__operationUnit" name="__operationUnit" type="hidden" value="@ViewBag.__operationUnit" />
    <input id="__userProfile" name="__userProfile" type="hidden" value="@ViewBag.__userProfile" />
    <input id="__localServerDate" name="__localServerDate" type="hidden" value="@ViewBag.__localServerDate" />
    <input id="__localTimeZoneOffset" name="__localTimeZoneOffset" type="hidden" value="@ViewBag.__localTimeZoneOffset" />
    <input id="__isCurrentDate" name="__isCurrentDate" type="hidden" value="@ViewBag.__isCurrentDate" />
    <input id="__lastLoginDate" name="__lastLoginDate" type="hidden" value="@ViewBag.__lastLoginDate" />   <!-- 20170815 - last login date-->
    <!-- 20170815 - last login date-->
    @RenderBody()
    <div ui-view id="app" ng-class="{'app-mobile' : app.isMobile, 'app-navbar-fixed' : app.layout.isNavbarFixed, 'app-sidebar-fixed' : app.layout.isSidebarFixed, 'app-sidebar-closed':app.layout.isSidebarClosed, 'app-footer-fixed':app.layout.isFooterFixed}"></div>

    <!-- jQuery -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Fastclick -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- Angular -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular/angular.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-cookies/angular-cookies.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-animate/angular-animate.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-touch/angular-touch.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
    <!-- Angular storage -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/ngstorage/ngStorage.min.js"></script>
    <!-- Angular Translate -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-translate/angular-translate.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-translate-loader-url/angular-translate-loader-url.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-translate-loader-static-files/angular-translate-loader-static-files.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-translate-storage-local/angular-translate-storage-local.min.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-translate-storage-cookie/angular-translate-storage-cookie.min.js"></script>
    <!-- oclazyload -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/oclazyload/dist/ocLazyLoad.min.js"></script>
    <!-- breadcrumb -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-breadcrumb/dist/angular-breadcrumb.min.js"></script>
    <!-- UI Bootstrap -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
    <!-- Loading Bar -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-loading-bar/build/loading-bar.min.js"></script>
    <!-- Angular Scroll -->
    <script src="~/Areas/Admin/Scripts/spa_admin/bower_components/angular-scroll/angular-scroll.min.js"></script>
    <!-- Toastr - Notification message -->
    <script src="~/Areas/Admin/Scripts/spa_admin/vendors/js/toastr.js"></script>
    <!-- Clip-Two Scripts -->
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/app.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/main.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/config.constant.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/config.router.js"></script>
    <!-- Clip-Two Directives -->
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/toggle.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/perfect-scrollbar.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/empty-links.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/sidebars.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/off-click.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/full-height.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/panel-tools.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/char-limit.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/dismiss.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/compare-to.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/select.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/messages.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/chat.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/sparkline.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/touchspin.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/directives/file-upload.js"></script>

    <!--123ChaChing Services-->
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/services/apiService.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/services/membershipService.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/services/orderService.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/services/notificationService.js"></script>

    <!-- Clip-Two Controllers -->
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/controllers/mainCtrl.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/controllers/inboxCtrl.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/controllers/bootstrapCtrl.js"></script>
    <script src="~/Areas/Admin/Scripts/spa_admin/assets/js/controllers/bootstrapCtrl.js"></script>
</body>
</html>